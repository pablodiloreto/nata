@model IdentityUser

@{
    ViewData["Title"] = "User Details";
    Layout = "_LayoutConfigurations";
}

<h3>Ticket Impact details for <strong>@Html.DisplayFor(model => model.UserName)</strong></h3>
<p>View details about this User.</p>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayFor(model => model.Email)
        </dt>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
